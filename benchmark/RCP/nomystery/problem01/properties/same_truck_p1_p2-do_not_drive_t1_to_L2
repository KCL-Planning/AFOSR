property same_truck_p1_p2-do_not_drive_t1_to_L2
 ||  &&  && p1_p2_delwithT0  ! p1_p2_delwithT1 d_T1_to_L2  &&  &&  ! p1_p2_delwithT0 p1_p2_delwithT1 d_T1_to_L2